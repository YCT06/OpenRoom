<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - OpenRoom.Web</title>
    <!-- bootstrap -->
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <!-- swiper -->
    <link rel="stylesheet" href="~/lib/swiper/swiper-bundle.min.css" />
    <!-- font-awesome -->
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.css" />
    <!-- flatpickr -->
    <link rel="stylesheet" href="~/lib/flatpickr/dist/flatpickr.min.css" />
    <!-- leaflet -->
    <link rel="stylesheet" href="~/lib/leaflet/dist/leaflet.css" />

   
    <!--googleapis-->
    <link rel="stylesheet" href="~/css/googleapis/googleapis.css" />
    <!--googleapis02-->
    <link rel="stylesheet" href="~/css/googleapis/googleapis02.css" />

    <!--datatable-->
    <link rel="stylesheet" href="~/css/cloudflare/cloudflare.css" />
    <link rel="stylesheet" href="~/lib/datatables.net-bs5/css/dataTables.bootstrap5.css" /> 

    @* <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/2.0.1/css/dataTables.bootstrap5.css"> *@

    <!-- _LayouCreateHosting.css -->
    <link href="~/css/hostingCSS/hosting02.css" rel="stylesheet" />

    @await RenderSectionAsync("topCSS", required: false)
    @await RenderSectionAsync("topJS", required: false)
</head>
<body>
    @await RenderSectionAsync("advertise", required: false)
    <header class="header-set container">
        <div class="header-info">
            <div class="brand-icon col">
                <div class="d-flex">
                    <a asp-controller="Home" asp-action="Index">
                        <img height="auto" width="80px" src="~/images/_Layout/icon/_logo.jpg" alt="header-icon" />
                    </a>
                </div>
            </div>
            @await RenderSectionAsync("headSpace", required: false)
            @if (Context.User.Identity.IsAuthenticated)
            {
                <partial name="_HostingMenuPartial" />
                <partial name="_HostingMemberBtnLoginPartial" />
            }
            else
            {
                <partial name="_MemberBtnLogoutPartial" />
            }
        </div>
        @await RenderSectionAsync("searchBar", required: false)
    </header>

    @await RenderSectionAsync("phoneHead", required: false)

    <main role="main">
        @RenderBody()
    </main>


    @if (false)
    {
        <partial name="_PhoneNavLogoutPartial" />
    }
    else
    {
        <partial name="_HostingPhoneNavLoginPartial" />
    }
    @await RenderSectionAsync("phoneNav", required: false)



    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/swiper/swiper-bundle.min.js"></script>
    <script src="~/lib/flatpickr/dist/flatpickr.min.js"></script>
    <script src="~/lib/leaflet/dist/leaflet.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/font-awesome/js/all.min.js"></script>
    <script src="~/lib/vue/vue.global.js"></script> 

    <!--datatabe-->
    
    <script src="~/js/cloudflare/cloudflare.js"></script>
    <script src="~/js/datatables/datatables.js"></script>
    <script src="~/lib/datatables.net-bs5/js/dataTables.bootstrap5.js"></script>
    <script src="~/js/jsdelivr/jsdelivr.js"></script>  

    @* <script src="https://code.jquery.com/jquery-3.7.1.js"></script> *@
    
    @* <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.0/js/bootstrap.bundle.min.js"></script> *@
    
    @* <script src="https://cdn.datatables.net/2.0.1/js/dataTables.js"></script> *@
   
    @* <script src="https://cdn.datatables.net/2.0.1/js/dataTables.bootstrap5.js"></script> *@

   @*  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script> *@

    
    @await RenderSectionAsync("Scripts", required: false)
    @await RenderSectionAsync("endJS", required: false)

</body>



</html>
